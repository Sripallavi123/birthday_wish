{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/birthday.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/birthday.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}